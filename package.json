{
  "name": "avatar",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "PORT=3100 next start",
    "lint": "next lint"
  },
  "dependencies": {
    "gsap": "^3.12.5",
    "js-cookie": "^3.0.5",
    "next": "15.1.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "eslint": "^9",
